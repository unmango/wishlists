{
	"name": "unmango-wishlists",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "conc \"npm run watch\" \"vite\"",
		"build": "npm run compile && tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "npm run compile && vite preview",
		"compile": "graphql-codegen",
		"watch": "graphql-codegen -w",
		"schema": "mkdir -p src/__generated__ && cp ../../api/schema.graphql src/__generated__"
	},
	"dependencies": {
		"@apollo/client": "^3.8.7",
		"graphql": "^16.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.4.0",
		"@graphql-codegen/cli": "^5.0.0",
		"@graphql-codegen/client-preset": "^4.1.0",
		"@graphql-typed-document-node/core": "^3.2.0",
		"@parcel/watcher": "^2.3.0",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^20.9.2",
		"@types/react": "^18.2.37",
		"@types/react-dom": "^18.2.15",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"concurrently": "^8.2.2",
		"eslint": "^9.4.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.4",
		"typescript": "^5.4.5",
		"typescript-eslint": "^8.0.0-alpha.24",
		"vite": "^5.0.0"
	}
}
